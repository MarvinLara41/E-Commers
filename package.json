{
	"name": "e-commers",
	"version": "1.0.0",
	"description": "A fully functional E-Commers site that allows users to view, remove, and purchase items from the site.",
	"main": "server.js",
	"engines": {
		"node": "12.16.2",
		"npm": "6.14.4"
	},
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
		"client": "cd client && npm run start",
		"seed": "node scripts/seedDB.js",
		"install": "cd client && npm install",
		"build": "cd client && npm run build",
		"heroku-postbuild": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MarvinLara41/E-Commers.git"
	},
	"keywords": [],
	"author": "Marvin Lara",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/MarvinLara41/E-Commers/issues"
	},
	"homepage": "https://github.com/MarvinLara41/E-Commers#readme",
	"proxy": "http://127.0.0.1:5000",
	"dependencies": {
		"body-parser": "^1.19.0",
		"concurrently": "^5.2.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.9.26",
		"multer": "^1.4.2"
	},
	"devDependencies": {
		"nodemon": "^2.0.4"
	}
}
